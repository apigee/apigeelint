{
  "name": "apigeelint",
  "version": "2.13.0",
  "description": "Node module to lint and Apigee Edge bundle.",
  "main": "index.js",
  "bin": {
    "apigeelint": "./cli.js"
  },
  "scripts": {
    "badge": "mocha --reporter mocha-badge-generator",
    "coverage": "nyc mocha",
    "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apigee/apigeelint"
  },
  "keywords": [
    "API",
    "bundle",
    "lint",
    "linter",
    "Apigee",
    "Edge"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apigee/apigeelint/issues"
  },
  "private": false,
  "dependencies": {
    "@xmldom/xmldom": "^0.7.4",
    "babel-code-frame": "latest",
    "chalk": "4.1.2",
    "commander": "^2.9.0",
    "debug": "latest",
    "decache": "latest",
    "decompress": "latest",
    "eslint": "latest",
    "js-yaml": "latest",
    "jshint": "^2.10.1",
    "jsonschema": "latest",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "node-find-folder": ">=0.1.25",
    "path": ">= 0.11.14",
    "pdfmake": "latest",
    "pluralize": "latest",
    "request": "latest",
    "strip-ansi": "^6.0.1",
    "table": "latest",
    "text-table": "latest",
    "util": "^0.10.3",
    "xpath": "latest"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "codacy-coverage": "^2.0.2",
    "es6-template-strings": "^2.0.1",
    "mocha": "^9.1.3",
    "mocha-badge-generator": "^0.9.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "resolutions": {
    "nth-check": "^2.0.1",
    "uuid": "^7.0.3"
  },
  "engines": {
    "node": "^12.22.6"
  }
}
