<GenerateJWT name='GenerateJWT-Signed-RS256-Simple'>
  <Algorithm>RS256</Algorithm>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <!-- RS256 must have a PrivateKey -->
  <!-- RS256 must not have a SecretKey -->
  <SecretKey>
    <Value ref='private.secretkey'/>
    <Id>cb24a396-4414-4793-b4ce-90333b694bff</Id>
  </SecretKey>

  <Subject>subject-subject-subject</Subject>
  <Audience>audience-here</Audience>
  <ExpiresIn>8h</ExpiresIn>
  <AdditionalClaims>
    <Claim name='additional-claim-name'>additional-claim-value-goes-here</Claim>
  </AdditionalClaims>
  <OutputVariable>output_variable_name</OutputVariable>
</GenerateJWT>
